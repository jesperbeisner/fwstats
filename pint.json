{
    "preset": "symfony",
    "rules": {
        "declare_strict_types": true,
        "strict_comparison": true,
        "yoda_style": false,
        "global_namespace_import": false,
        "new_with_parentheses": true,
        "phpdoc_no_alias_tag": false,
        "concat_space": {
            "spacing": "one"
        },
        "phpdoc_align": {
            "align": "left"
        }
    },
    "notName": [
        "phpstan-baseline.php"
    ]
}
