{
    "name": "jesperbeisner/fwstats",
    "type": "project",
    "description": "Statistics website for the browsergame freewar.de.",
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.22",
        "laravel/octane": "^2.12.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16.0",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.6.0",
        "laravel/pint": "^1.24.0",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "phpstan/phpstan": "^2.1.22",
        "phpstan/phpstan-strict-rules": "^2.0.6",
        "phpunit/phpunit": "^12.3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan config:clear",
            "@php artisan clear-compiled",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
